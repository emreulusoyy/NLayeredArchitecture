@model List<EntityLayer.Concrete.Slider>

	

	<section class="ftco-section">
		<div class="container">
			<div class="row">
				<div class="col-md-12 text-center">
				</div>
				<div class="col-md-12">
					<div class="slider-hero">
						<div class="featured-carousel owl-carousel">
							@foreach(var x in Model){
							<div class="item">
								<div class="work">
									<div class="img d-flex align-items-center justify-content-center" style="background-image: @x.SliderImage">
										<div class="text text-center">
											<h3>@x.SliderBaslik</h3>
										</div>
									</div>
								</div>
							</div>
							}
						</div>

					</div>
				</div>
			</div>
		</div>
	</section>

<script src="~/carousel-10/js/jquery.min.js"></script>
<script src="~/carousel-10/js/popper.js"></script>
<script src="~/carousel-10/js/bootstrap.min.js"></script>
<script src="~/carousel-10/js/owl.carousel.min.js"></script>
<script src="~/carousel-10/js/main.js"></script>
